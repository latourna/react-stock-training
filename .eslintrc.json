{
  "extends": "airbnb",
  "plugins": [
    "react",
    "jsx-a11y",
    "import"
  ],
  "parser": "babel-eslint",
  "rules": {
    "no-console": [
      "off"
    ],
    "react/jsx-filename-extension": [
      "off"
    ],
    "react/jsx-no-bind": [
      "off"
    ],
    "react/prefer-stateless-function": [
      "off"
    ]
  }
}
